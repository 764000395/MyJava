package 第四章.例二;

public class SwitchExample {
	private int grade;
	public char dengji;

	public void setGrade(int grade) {
		this.grade = grade;
	}

	public int getGrade() {
		return this.grade;
	}

	public char getDengji() {
		int forMateGrade = this.grade / 10;
		switch (forMateGrade) {
		case 10:
		case 9:
			dengji = 'A';
			break;
		case 8:
			dengji = 'B';
			break;
		case 7:
			dengji = 'C';
			break;

		case 6:
			dengji = 'D';
			break;
		default:
			dengji = 'E';
		}
		return dengji;
	}
}
